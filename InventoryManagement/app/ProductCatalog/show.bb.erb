<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @productcatalog.Name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @productcatalog.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @productcatalog.Name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">SKU</td>
        <td class="itemValue"><%= @productcatalog.SKU%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Image</td>
        <td class="itemValue"><%= @productcatalog.Image%></td>
      </tr>
      
    </table>
  </div>

</div>
